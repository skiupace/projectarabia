{
	"name": "projectarabia",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"serve": "vite preview",
		"generate-routes": "tsr generate",
		"push-db": "drizzle-kit push",
		"watch-routes": "tsr watch",
		"test": "vitest run",
		"deploy": "bun run generate-routes && bun run build && wrangler deploy",
		"preview": "bun run generate-routes && bun run build && vite preview",
		"cf-typegen": "wrangler types",
		"lint": "biome lint --write ./src",
		"format": "biome format --write ./src",
		"knip": "knip",
		"seed": "tsx seed/index.ts",
		"seed:reset": "tsx seed/index.ts --reset"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.13.8",
		"@libsql/client": "^0.15.15",
		"@marsidev/react-turnstile": "^1.3.1",
		"@paralleldrive/cuid2": "^3.0.4",
		"@tailwindcss/vite": "^4.0.6",
		"@tanstack/react-devtools": "^0.7.0",
		"@tanstack/react-form": "^1.23.8",
		"@tanstack/react-query": "^5.90.5",
		"@tanstack/react-router": "^1.132.0",
		"@tanstack/react-router-devtools": "^1.132.0",
		"@tanstack/react-start": "^1.132.0",
		"@tanstack/router-plugin": "^1.132.0",
		"@unpic/react": "^1.0.1",
		"bcryptjs": "^3.0.2",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.7",
		"lucide-react": "^0.552.0",
		"nanoid": "^5.1.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"resend": "^6.4.0",
		"vite-tsconfig-paths": "^5.1.4",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@tanstack/router-cli": "^1.133.36",
		"@types/bcryptjs": "^3.0.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@vitejs/plugin-react": "^5.0.4",
		"better-sqlite3": "^12.4.1",
		"drizzle-kit": "^0.31.6",
